{{if eq .Name "write"}} <span style="color: #06b6d4;">← writing to tap device or vsock</span>{{end}}
{{if eq .Name "readv"}} <span style="color: #06b6d4;">← reading network packets from tap</span>{{end}}
{{if eq .Name "writev"}} <span style="color: #06b6d4;">← writing network packets to tap</span>{{end}}
{{if eq .Name "ppoll"}} <span style="color: #06b6d4;">← FC polling for I/O events</span>{{end}}
{{if eq .Name "futex"}} <span style="color: #06b6d4;">← thread synchronization</span>{{end}}
</li>
{{end}}
</ul>
{{end}}

