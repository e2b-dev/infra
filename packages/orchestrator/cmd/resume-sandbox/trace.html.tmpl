{{if .PageAnalysis.OrderingChart}}
<div class="ordering-chart-section">
<h3>Ordering Deviation Charts</h3>
<p class="freq-desc">Shows how much each page fault deviates from its expected position. Y-axis: deviation (positive = appeared later than average, negative = earlier).</p>

<div class="chart-stack">
<div class="chart-cell-full">
<h4>Per-Run Instantaneous Deviation</h4>
<div class="ordering-chart-container">
<canvas id="ordering-chart" width="1760" height="500" style="width:880px;height:250px"></canvas>
</div>
<div class="chart-legend" id="chart-legend"></div>
</div>
<div class="chart-cell-full">
<h4>Per-Run Cumulative Deviation (divergence from core pattern)</h4>
<div class="ordering-chart-container">
<canvas id="cumulative-chart" width="1760" height="500" style="width:880px;height:250px"></canvas>
</div>
<div class="chart-legend" id="cumulative-legend"></div>
</div>
</div>
</div>
<script>
(function() {
  const data = [
    {{range $i, $run := .PageAnalysis.OrderingChart}}
    { run: {{$run.Run}}, points: [{{range .Points}}{x:{{.X}},y:{{printf "%.2f" .Y}},page:{{.Page}},exp:{{printf "%.1f" .ExpectedX}}},{{end}}] },
    {{end}}
  ];
  
  const colors = ['#22c55e', '#3b82f6', '#f97316', '#a855f7', '#ec4899', '#14b8a6', '#eab308', '#ef4444', '#06b6d4', '#84cc16'];
  
  // High-DPI canvas setup
  function setupHiDPICanvas(canvas, width, height) {
    const ctx = canvas.getContext('2d');
    ctx.scale(2, 2);
    return ctx;
  }
  
  // Find data bounds for instantaneous deviation
  let maxX = 0, maxY = 0;
  data.forEach(run => {
    run.points.forEach(p => {
      if (p.x > maxX) maxX = p.x;
      if (Math.abs(p.y) > maxY) maxY = Math.abs(p.y);
    });
  });
  maxY = Math.max(maxY, 1);
  maxX = Math.max(maxX, 1);
  
  // Build "core" ordering: for each page, find its median position across all runs
  const pagePositions = {};
  data.forEach(run => {
    run.points.forEach(p => {
      if (!pagePositions[p.page]) pagePositions[p.page] = [];
      pagePositions[p.page].push(p.x);
    });
  });
  
  // Core position = median of all positions for that page
  const corePosition = {};
  Object.keys(pagePositions).forEach(page => {
    const positions = pagePositions[page].sort((a, b) => a - b);
    const mid = Math.floor(positions.length / 2);
    corePosition[page] = positions.length % 2 === 0 
      ? (positions[mid - 1] + positions[mid]) / 2 
      : positions[mid];
  });
  
  // Calculate cumulative variance for each run (deviation from core pattern)
  // For each fault, add |actual_position - core_position| to running total
  const cumulativeData = data.map(run => {
    let cumVariance = 0;
    const cumPoints = run.points.map(p => {
      const expected = corePosition[p.page] || p.exp;
      const deviation = Math.abs(p.x - expected);
      cumVariance += deviation;
      return { x: p.x, y: cumVariance, page: p.page, deviation: deviation };
    });
    return { run: run.run, points: cumPoints, finalVariance: cumVariance };
  });
  
  // Find max cumulative variance
  let maxCumY = 0;
  cumulativeData.forEach(run => {
    run.points.forEach(p => {
      if (p.y > maxCumY) maxCumY = p.y;
    });
  });
  maxCumY = Math.max(maxCumY, 1);
  
  const CHART_W = 880, CHART_H = 250;
  const margin = { top: 25, right: 30, bottom: 35, left: 60 };
  
  // ===== Chart 1: Instantaneous Deviation (Scatter) =====
  const canvas1 = document.getElementById('ordering-chart');
  const ctx1 = setupHiDPICanvas(canvas1, CHART_W, CHART_H);
  const legend1 = document.getElementById('chart-legend');
  
  const w1 = CHART_W - margin.left - margin.right;
  const h1 = CHART_H - margin.top - margin.bottom;
  const centerY1 = margin.top + h1 / 2;
  
  const scaleX1 = (x) => margin.left + (x / maxX) * w1;
  const scaleY1 = (y) => centerY1 - (y / maxY) * (h1 / 2 - 5);
  
  ctx1.fillStyle = '#161616';
  ctx1.fillRect(margin.left, margin.top, w1, h1);
  
  // Zero line
  ctx1.strokeStyle = '#444';
  ctx1.lineWidth = 1;
  ctx1.beginPath();
  ctx1.moveTo(margin.left, centerY1);
  ctx1.lineTo(margin.left + w1, centerY1);
  ctx1.stroke();
  
  // Grid
  ctx1.strokeStyle = '#2a2a2a';
  ctx1.setLineDash([4, 4]);
  for (let i = -2; i <= 2; i++) {
    if (i === 0) continue;
    const y = scaleY1(i * maxY / 2);
    ctx1.beginPath();
    ctx1.moveTo(margin.left, y);
    ctx1.lineTo(margin.left + w1, y);
    ctx1.stroke();
  }
  ctx1.setLineDash([]);
  
  // Y-axis
  ctx1.strokeStyle = '#555';
  ctx1.beginPath();
  ctx1.moveTo(margin.left, margin.top);
  ctx1.lineTo(margin.left, margin.top + h1);
  ctx1.stroke();
  
  // Labels
  ctx1.fillStyle = '#737373';
  ctx1.font = '11px -apple-system, BlinkMacSystemFont, sans-serif';
  ctx1.textAlign = 'center';
  ctx1.fillText('Fault Order', margin.left + w1/2, CHART_H - 8);
  ctx1.textAlign = 'right';
  ctx1.fillText('0', margin.left - 6, centerY1 + 4);
  ctx1.fillText('+' + Math.round(maxY), margin.left - 6, margin.top + 12);
  ctx1.fillText('-' + Math.round(maxY), margin.left - 6, margin.top + h1 - 4);
  
  // Draw scatter points
  data.forEach((run, runIdx) => {
    const color = colors[runIdx % colors.length];
    ctx1.fillStyle = color;
    ctx1.globalAlpha = 0.5;
    run.points.forEach(p => {
      ctx1.beginPath();
      ctx1.arc(scaleX1(p.x), scaleY1(p.y), 2.5, 0, Math.PI * 2);
      ctx1.fill();
    });
  });
  ctx1.globalAlpha = 1;
  
  // Legend
  const maxLegend = Math.min(data.length, 10);
  for (let i = 0; i < maxLegend; i++) {
    const span = document.createElement('span');
    span.className = 'legend-item';
    span.innerHTML = '<span class="legend-dot" style="background:' + colors[i % colors.length] + '"></span>Run ' + data[i].run;
    legend1.appendChild(span);
  }
  if (data.length > 10) {
    const span = document.createElement('span');
    span.className = 'legend-item';
    span.textContent = '+' + (data.length - 10) + ' more';
    legend1.appendChild(span);
  }
  
  // ===== Chart 2: Cumulative Variance (Lines going up from baseline) =====
  const canvas2 = document.getElementById('cumulative-chart');
  const ctx2 = setupHiDPICanvas(canvas2, CHART_W, CHART_H);
  const legend2 = document.getElementById('cumulative-legend');
  
  const w2 = CHART_W - margin.left - margin.right;
  const h2 = CHART_H - margin.top - margin.bottom;
  const baselineY2 = margin.top + h2; // Bottom of chart = 0 variance
  
  // Scale: 0 at bottom, maxCumY at top
  const scaleX2 = (x) => margin.left + (x / maxX) * w2;
  const scaleY2 = (y) => baselineY2 - (y / maxCumY) * (h2 - 10);
  
  ctx2.fillStyle = '#161616';
  ctx2.fillRect(margin.left, margin.top, w2, h2);
  
  // Baseline (0 variance = perfect match with core)
  ctx2.strokeStyle = '#22c55e';
  ctx2.lineWidth = 2;
  ctx2.beginPath();
  ctx2.moveTo(margin.left, baselineY2);
  ctx2.lineTo(margin.left + w2, baselineY2);
  ctx2.stroke();
  
  // Grid lines
  ctx2.strokeStyle = '#2a2a2a';
  ctx2.lineWidth = 1;
  ctx2.setLineDash([4, 4]);
  for (let i = 1; i <= 4; i++) {
    const y = scaleY2(i * maxCumY / 4);
    ctx2.beginPath();
    ctx2.moveTo(margin.left, y);
    ctx2.lineTo(margin.left + w2, y);
    ctx2.stroke();
  }
  ctx2.setLineDash([]);
  
  // Y-axis
  ctx2.strokeStyle = '#555';
  ctx2.beginPath();
  ctx2.moveTo(margin.left, margin.top);
  ctx2.lineTo(margin.left, margin.top + h2);
  ctx2.stroke();
  
  // Labels
  ctx2.fillStyle = '#737373';
  ctx2.font = '11px -apple-system, BlinkMacSystemFont, sans-serif';
  ctx2.textAlign = 'center';
  ctx2.fillText('Fault Order', margin.left + w2/2, CHART_H - 8);
  ctx2.textAlign = 'right';
  ctx2.fillText('0', margin.left - 6, baselineY2 - 3);
  ctx2.fillText(Math.round(maxCumY / 2), margin.left - 6, scaleY2(maxCumY / 2) + 4);
  ctx2.fillText(Math.round(maxCumY), margin.left - 6, margin.top + 12);
  
  // Y-axis label
  ctx2.save();
  ctx2.translate(12, margin.top + h2 / 2);
  ctx2.rotate(-Math.PI / 2);
  ctx2.textAlign = 'center';
  ctx2.fillText('Cumulative Variance', 0, 0);
  ctx2.restore();
  
  // Draw cumulative lines for each run (starting from bottom-left)
  cumulativeData.forEach((run, runIdx) => {
    if (run.points.length < 1) return;
    const color = colors[runIdx % colors.length];
    ctx2.strokeStyle = color;
    ctx2.lineWidth = 1.5;
    ctx2.globalAlpha = 0.8;
    ctx2.beginPath();
    // Start at origin (0, 0)
    ctx2.moveTo(scaleX2(0), scaleY2(0));
    run.points.forEach(p => {
      ctx2.lineTo(scaleX2(p.x), scaleY2(p.y));
    });
    ctx2.stroke();
  });
  ctx2.globalAlpha = 1;
  
  // Sort runs by final variance for legend
  const sortedRuns = [...cumulativeData].sort((a, b) => a.finalVariance - b.finalVariance);
  const bestRun = sortedRuns[0];
  const worstRun = sortedRuns[sortedRuns.length - 1];
  
  // Legend with insights
  legend2.innerHTML = '<span class="legend-item" style="color:#a3a3a3">Lines show cumulative ordering variance from core pattern. Lower = more consistent.</span>' +
    '<span class="legend-item"><span class="legend-dot" style="background:' + colors[bestRun.run % colors.length] + '"></span>Best: Run ' + bestRun.run + ' (' + Math.round(bestRun.finalVariance) + ')</span>' +
    '<span class="legend-item"><span class="legend-dot" style="background:' + colors[worstRun.run % colors.length] + '"></span>Worst: Run ' + worstRun.run + ' (' + Math.round(worstRun.finalVariance) + ')</span>';
})();
</script>
<style>
.chart-stack { display: flex; flex-direction: column; gap: 20px; }
.chart-cell-full { width: 100%; }
.chart-cell-full h4 { font-size: 0.7rem; color: #a3a3a3; margin-bottom: 8px; font-weight: 500; }
</style>
{{end}}

</div>
{{end}}

