        "p95",
        "p99"
      ],
      "displayMode": "table",
      "placement": "bottom",
      "showLegend": true
    },
    "tooltip": {
      "hideZeros": false,
      "mode": "single",
      "sort": "none"
    }
  },
  "pluginVersion": "12.0.0-85518.patch7-85777",
  "targets": [
    {
      "datasource": {
        "type": "tempo",
        "uid": "grafanacloud-traces"
      },
      "filters": [
        {
          "id": "span-name",
          "operator": "=",
          "scope": "span",
          "tag": "name",
