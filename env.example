# E2B Infrastructure - Example Environment Configuration
# Copy this file to .env.local and adjust values as needed

# Environment
ENVIRONMENT=local
TERRAFORM_ENVIRONMENT=local

# GCP Configuration (dummy values for local dev)
GCP_PROJECT_ID=e2b-local
GCP_REGION=us-central1
GCP_ZONE=us-central1-a
DOMAIN_NAME=localhost
PREFIX=local-

# Database Connections
POSTGRES_CONNECTION_STRING=postgres://e2b:e2b_password@localhost:5432/e2b?sslmode=disable
CLICKHOUSE_CONNECTION_STRING=clickhouse://e2b:clickhouse_password@localhost:9000/e2b_analytics
REDIS_URL=redis://localhost:6379

# Authentication & Security
SUPABASE_JWT_SECRETS={"jwt_secret":"your-super-secret-jwt-key-at-least-32-chars","service_role_key":"your-service-role-key"}
ADMIN_TOKEN=admin-secret-token
EDGE_SECRET=edge-secret-key
SANDBOX_ACCESS_TOKEN_HASH_SEED=sandbox-hash-seed

# Service Configuration
ORCHESTRATOR_PORT=5008
TEMPLATE_MANAGER_PORT=5009
API_PORT=3000
EDGE_PORT=3001
PROXY_PORT=3002
DOCKER_REVERSE_PROXY_PORT=5000

# Storage
TEMPLATE_BUCKET_NAME=e2b-templates
FC_ENV_PIPELINE_BUCKET_NAME=e2b-fc-pipeline
FC_KERNELS_BUCKET_NAME=e2b-fc-kernels
FC_VERSIONS_BUCKET_NAME=e2b-fc-versions

# Google Service Account (base64 encoded JSON) - dummy for local
GOOGLE_SERVICE_ACCOUNT_BASE64=eyJ0eXBlIjoic2VydmljZV9hY2NvdW50In0=

# Docker Registry
GCP_DOCKER_REPOSITORY_NAME=e2b-local-docker

# Telemetry & Monitoring (optional for local)
OTEL_TRACING_PRINT=true
OTEL_COLLECTOR_GRPC_ENDPOINT=localhost:4317
LOGS_COLLECTOR_ADDRESS=localhost:3100
LOKI_ADDRESS=http://localhost:3100

# Analytics (leave empty for local)
POSTHOG_API_KEY=
ANALYTICS_COLLECTOR_HOST=
ANALYTICS_COLLECTOR_API_TOKEN=
LAUNCH_DARKLY_API_KEY=

# Nomad (not used in Docker Compose)
NOMAD_ADDRESS=http://localhost:4646
NOMAD_TOKEN=dummy-token

# Resource Configuration
CLIENT_PROXY_COUNT=1
CLIENT_PROXY_CPU_COUNT=1
CLIENT_PROXY_RESOURCES_MEMORY_MB=512
CLICKHOUSE_RESOURCES_CPU_COUNT=2
CLICKHOUSE_RESOURCES_MEMORY_MB=2048

# Feature Flags
ALLOW_SANDBOX_INTERNET=true
FORCE_STOP=false 